        android:theme="@style/AppTheme">
        <activity android:name=".img_upload"></activity>
        <activity android:name=".email_verify" />
        <activity android:name=".Razorpay" />
        <activity android:name=".Add_money" />
        <activity android:name=".MainActivity" />
