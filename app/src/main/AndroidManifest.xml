        <activity
            android:name=".KYC_update"
            android:label="KYC UPDATE" />
        <activity
            android:name=".BankDetails_update"
            android:label="BANK DETAILS" />
        <activity android:name=".LiteTradeSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <meta-data
            android:name="com.razorpay.ApiKey"
            android:value="rzp_test_QybM6EEM11HTsU" />
